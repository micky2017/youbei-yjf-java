<import src="../../common/TitleBar.ux"></import>
<template>
    <div class="web-body">
        <TitleBar title="幸运抽奖"></TitleBar>
        <web if="webUrl" trustedurl='{{trustedurl}}' src="{{webUrl}}"></web>
    </div>
</template>
<style lang="less" scoped>
    .web-body{
        width: 100%;
        height: 100vh;
        flex-direction: column;
        background-color: #f4f4f4;
        padding-top: 100px;
        align-items: center;
        >web{
            width: 100%;
            height: 100vh;
        }
    }
</style>
<script>
import storage from '@system.storage';
export default Custom_page({
    private: {
        webUrl:'',
        trustedurl:['https://api.ubaycn.com/lottery'],
    },
    // http://192.168.1.111:8020/jumpToYjf/index.html
    onInit() {
        storage.get({
            key:'_id',
            success:(data) => {
                if(data){
                    this.webUrl = `https://api.ubaycn.com/lottery?sid=${data}`
                }else{
                    this.webUrl = 'https://api.ubaycn.com/lottery'
                }
            },
            fail:() => {
                    this.webUrl = 'https://api.ubaycn.com/lottery'
            }
        })
    },
    onReady() {
        console.log(this.webUrl)
    },
})
</script>

